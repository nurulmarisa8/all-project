<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.beautra.controller.SellerDashboardController" padding="10">

    <Label text="Seller Dashboard" style="-fx-font-size: 24px;"/>
    <ListView fx:id="productListView" prefHeight="400"/>

    <HBox spacing="10">
        <Button text="Tambah Produk" onAction="#handleAddProduct"/>
        <Button text="Edit Produk" onAction="#handleEditProduct"/>
    </HBox>
</VBox>
